{
    "author": "GSM",
    "background": {
        "page": "background.html",
        "persistent": true
    },
    "description": "Restore Classic YouTube!",
    "icons": {
        "16": "images/yt_classic16.png",
        "32": "images/yt_classic32.png",
        "48": "images/yt_classic48.png",
        "128": "images/yt_classic128.png"
    },
    "manifest_version": 2,
    "name": "YouTube Classic",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "*://www.youtube.com/*"
    ],
    "version": "1.2",
    "-ms-preload": {
        "backgroundScript": "backgroundScriptsAPIBridge.js",
        "contentScript": "contentScriptsAPIBridge.js"
    }
}